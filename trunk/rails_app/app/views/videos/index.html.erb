<% for video in Video.all %>
  <tr>
    <td><%= image_tag video.thumbnail_url %></td>
    <td><%= video.embed_html(120, 120) %></td>
    <td><%= video.description %></td>
    <td><%= video.uid %></td>
    <td>
      <% if video.geotagged? %>
      <%= image_tag map_url(video) %>
      (<%= video.lat %>, <%= video.lng %>)
      <% end %>
    </td>
  </tr>
<% end %>
</table>