<h1>Listing videos</h1>

<table>
  <tr>
  </tr>

<% for video in @videos %>
  <tr>
    <td><%= video.title %></td>
    <td><%= video.address %></td>
    <td><%= video.lat %>, <%= video.lng %></td>
    <td><%= link_to 'Show', video_path(video) %></td>
    <td><%= link_to 'Destroy', video_path(video), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New video', new_video_path %>

